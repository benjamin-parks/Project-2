        <h4 class="center">Active Animal Profiles</h4>

        <div class="col s10 pet-table ">
          <table class="highlight">
            <thead>
              <tr class="grey lighten-3 tabpad">
                <th>ID</th>
                <th>Name</th>
                <th>Age</th>
                <th>Type</th>
                <th>Breed</th>
                <th>Delete</th>
              </tr>
            </thead>
      
            <tbody>
              {{#each petsArray}}
              <tr>
                <td>{{this.id}}</td>
                <td>{{this.pet_name}}</td>
                <td>{{this.pet_age}}</td>
                <td>{{this.pet_type}}</td>
                <td>{{this.pet_breed}}</td>
                <td><a class="btn-floating btn-small waves-effect waves-light red" onclick="deletePet({{this.id}})"><i class="material-icons">clear</i></a></td>
              </tr>
              {{/each}}
            </tbody>
            
          </table>
        </div>