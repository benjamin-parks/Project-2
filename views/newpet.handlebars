          <button id="addPetButton" class="btn waves-effect waves-light right" type="submit" name="action">Submit
            <i class="material-icons right">send</i>
          </button>
          <!-- <a class="waves-effect waves-light btn-small red"><i class="material-icons right">cancel</i>Cancel</a> -->
